{
  "name": "jailbreakchangelogs",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "fumadocs-mdx && bun --bun next dev --port 5500",
    "build": "fumadocs-mdx && bun --bun next build",
    "build:analyze": "ANALYZE=true bun --bun next build",
    "build:perf": "bun --bun scripts/build-performance.js",
    "analyze": "bun --bun next experimental-analyze",
    "start": "bun --bun next start",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "check": "bun run type-check && bun run lint",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "changelog:legacy": "bun scripts/gen-changelog.ts",
    "generate:changelog": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s -r 0",
    "release": "release-it",
    "prepare": "husky"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@floating-ui/dom": "^1.7.4",
    "@floating-ui/react": "^0.27.16",
    "@headlessui/react": "^2.2.9",
    "@iconify/react": "^6.0.2",
    "@iconify/types": "^2.0.0",
    "@marsidev/react-turnstile": "^1.4.1",
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@mui/lab": "^7.0.0",
    "@mui/material": "^7.3.6",
    "@mui/material-nextjs": "^7.3.6",
    "@mui/x-date-pickers": "^8.23.0",
    "@next/third-parties": "^16.1.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-virtual": "^3.13.14",
    "@types/canvas-confetti": "^1.9.0",
    "@types/mdx": "^2.0.13",
    "babel-plugin-react-compiler": "^1.0.0",
    "canvas-confetti": "^1.9.4",
    "chart.js": "^4.5.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-plugin-zoom": "^2.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.1",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.26",
    "fumadocs-core": "^16.4.2",
    "fumadocs-mdx": "^14.2.4",
    "geist": "^1.5.1",
    "motion": "^12.23.26",
    "next": "16.1.1",
    "nextjs-toploader": "^3.9.17",
    "react": "^19.2.3",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.3",
    "react-hot-toast": "^2.6.0",
    "react-markdown": "^10.1.0",
    "schema-dts": "^1.1.5",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@eslint/eslintrc": "^3.3.3",
    "@release-it/conventional-changelog": "^10.0.4",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/bun": "^1.3.5",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "conventional-changelog-cli": "^5.0.0",
    "daisyui": "^5.5.14",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "release-it": "^19.2.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "**/*": "prettier --write --ignore-unknown"
  },
  "browserslist": "> 1%",
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": false
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "CHANGELOG.md",
        "preset": "conventionalcommits"
      }
    }
  }
}
